{
  "name": "web-backend",
  "version": "0.1.0",
  "main": "server.js",
  "scripts": {
    "start": "node ./build/server.build.js",
    "test": "jest --testPathIgnorePatterns models node_modules client .git --runInBand",
    "test:watch": "jest --testPathIgnorePatterns models node_modules client .git --watchAll --runInBand",
    "test:models": "jest --testPathPattern models --watchAll",
    "server": "nodemon server.js",
    "format": "prettier --write '**/*.{js,jsx}'",
    "dev": "webpack --watch --mode=development",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "babel": "babel ./server.js -d build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/132nd-vWing-website/web-backend"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/132nd-vWing-website/web-backend/issues"
  },
  "dependencies": {
    "@sanity/client": "^0.132.10",
    "@sanity/image-url": "^0.132.8",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.4.13",
    "mysql": "^2.16.0",
    "nodemon": "^1.18.10",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "eslint": "^5.14.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.8.0",
    "prettier": "^1.16.4",
    "supertest": "^3.4.2",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  },
  "jest": {
    "testURL": "http://localhost/",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/client/"
    ]
  }
}
